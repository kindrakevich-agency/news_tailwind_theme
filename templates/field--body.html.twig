{#
/**
 * @file
 * Theme implementation for the body field.
 *
 * Available variables:
 * - items: List of all the field items
 * - element: The field element
 */
#}
{% if items %}
    {% for item in items %}
        <div class="text-gray-700 leading-relaxed">
            {{ item.content }}
        </div>
    {% endfor %}
{% endif %}
